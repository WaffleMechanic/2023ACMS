@page
@model _2023ACMS.Pages.Medias.AddMediaModel
@{
    ViewData["Title"] = "Add Media";
}

<form method="post">
    <table>
        <tr>
            <td><label asp-for="Media.Media1"></label><label>: </label></td>
            <td>
                <input asp-for="Media.Media1" />
                <span asp-validation-for="Media.Media1" />
            </td>
        </tr>
        <tr>
            <td></td>
            <td>
                <input asp-page-handler="Add" type="submit" value="Add" />
                <a asp-page="CancelMedia" class="btn">Cancel</a>
            </td>
        </tr>
    </table>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}